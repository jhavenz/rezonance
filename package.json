{
  "name": "rezonance",
  "version": "0.1.2",
  "description": "Signal-based cache invalidation for Laravel + React Query",
  "keywords": [
    "laravel",
    "react-query",
    "tanstack",
    "cache-invalidation",
    "signals",
    "kubb",
    "openapi"
  ],
  "author": "Jonathan Havens <jhavenz@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhavenz/rezonance.git"
  },
  "bugs": {
    "url": "https://github.com/jhavenz/rezonance/issues"
  },
  "homepage": "https://github.com/jhavenz/rezonance#readme",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./devtools": {
      "types": "./dist/devtools.d.ts",
      "import": "./dist/devtools.js",
      "require": "./dist/devtools.cjs"
    },
    "./kubb": {
      "types": "./dist/kubb.d.ts",
      "import": "./dist/kubb.js",
      "require": "./dist/kubb.cjs"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js",
      "require": "./dist/config.cjs"
    }
  },
  "files": [
    "dist",
    "bin/**/*.js"
  ],
  "sideEffects": false,
  "bin": {
    "rezonance": "./bin/resonance.js"
  },
  "scripts": {
    "build": "tsup",
    "build:cli": "bun build ./bin/resonance.ts --outfile ./bin/resonance.js --target=node --format=esm && bun build ./bin/commands/dev.ts --outfile ./bin/commands/dev.js --target=node --format=esm && bun build ./bin/commands/generate.ts --outfile ./bin/commands/generate.js --target=node --format=esm",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@kubb/cli": "^4.15.0",
    "@kubb/core": "^4.15.0",
    "@kubb/plugin-oas": "^4.15.0",
    "@kubb/plugin-react-query": "^4.15.0",
    "@kubb/plugin-ts": "^4.15.0",
    "@kubb/plugin-zod": "^4.15.0",
    "yargs": "^18.0.0"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-router": "^1.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/yargs": "^17.0.35",
    "@vitest/coverage-v8": "^4.0.18",
    "jsdom": "^28.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  }
}
