{
    "name": "@jhavenz/resonance",
    "version": "1.0.0",
    "description": "TypeScript client for Resonance Framework",
    "type": "module",
    "main": "./src/js/index.ts",
    "types": "./src/js/index.ts",
    "exports": {
        ".": "./src/js/index.ts",
        "./devtools": "./src/js/devtools/index.tsx",
        "./kubb": "./src/js/kubb.ts",
        "./client/*": "./src/js/client/*",
        "./hooks/*": "./src/js/hooks/*",
        "./types/*": "./src/js/types/*",
        "./config": "./src/config/index.ts"
    },
    "sideEffects": false,
    "bin": {
        "resonance": "./bin/resonance.js"
    },
    "scripts": {
        "build:cli": "bun build ./bin/resonance.ts --outfile ./bin/resonance.js --target=node --format=esm && bun build ./bin/commands/dev.ts --outfile ./bin/commands/dev.js --target=node --format=esm && bun build ./bin/commands/generate.ts --outfile ./bin/commands/generate.js --target=node --format=esm"
    },
    "files": [
        "src/js",
        "bin"
    ],
    "dependencies": {
        "@kubb/cli": "^4.15.0",
        "@kubb/core": "^4.15.0",
        "@kubb/plugin-oas": "^4.15.0",
        "@kubb/plugin-react-query": "^4.15.0",
        "@kubb/plugin-ts": "^4.15.0",
        "@kubb/plugin-zod": "^4.15.0",
        "yargs": "^18.0.0"
    },
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0",
        "@tanstack/react-query": "^5.0.0",
        "@tanstack/react-router": "^1.0.0",
        "@tanstack/react-devtools": "^0.9.0",
        "superjson": "^2.0.0",
        "ulid": "^3.0.0",
        "zod": "^4.0.0"
    },
    "peerDependenciesMeta": {
        "@tanstack/react-devtools": {
            "optional": true
        }
    },
    "keywords": [
        "laravel",
        "react",
        "signals",
        "tanstack",
        "spa",
        "resonance"
    ],
    "license": "MIT",
    "devDependencies": {
        "@types/yargs": "^17.0.35",
        "yargs": "^18.0.0"
    }
}
